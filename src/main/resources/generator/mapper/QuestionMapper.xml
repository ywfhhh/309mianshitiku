<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.mapper.QuestionMapper">

    <resultMap id="BaseResultMap" type="generator.domain.Question">
        <id property="id" column="id"/>
        <result property="title" column="title"/>
        <result property="content" column="content"/>
        <result property="tagList" column="tagList"/>
        <result property="answer" column="answer"/>
        <result property="userId" column="userId"/>
        <result property="editTime" column="editTime"/>
        <result property="createTime" column="createTime"/>
        <result property="updateTime" column="updateTime"/>
        <result property="isDelete" column="isDelete"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,title,content,tagList,answer,userId,
        editTime,createTime,updateTime,isDelete
    </sql>
</mapper>
